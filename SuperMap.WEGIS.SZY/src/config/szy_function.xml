			<item>
				<name>全幅</name>
				<key>FullExtent</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
			<item>
				<name>测距</name>
				<key>Distance</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
			<item>
				<name>测面</name>
				<key>Area</key>
				<securitycontroler>false</securitycontroler>
