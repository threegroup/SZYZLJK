<?xml version="1.0" encoding="UTF-8"?>
<functions>
	<!-- 预加载模块 -->
	<initwidget>
		<item>
			<name>显示控制</name>
			<key>BaseMapControl</key>
			<url>widgets/basemap/BaseMapWidget.swf</url>
			<mutex>false</mutex>
			<top>35</top>
			<right>5</right>
		</item>
	</initwidget>
	<menu>
		<mapmenu>
			<item>
				<name>水体查询</name>
				<key>WaterBodyQuery</key>
				<url></url>
				<mutex>true</mutex>
				<init></init>
				<selected></selected>
			</item>
			<item>
				<name>地图服务</name>
				<key>MapService</key>
				<url></url>
				<mutex>true</mutex>
				<init></init>
				<selected></selected>
			</item>
			<item>
				<name>用户登录</name>
				<key>UserLogin</key>
				<url></url>
				<mutex>true</mutex>
				<init></init>
				<selected></selected>
			</item>
		</mapmenu>
	</menu>
	<assistmenu>
	</assistmenu>
	<widgets>
		<mapsearch>
		</mapsearch>
	</widgets>
	<!--工具条 -->
	<tool>
		<maptool>
			<item>
				<name>漫游</name>
				<key>Pan</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
			<item>
				<name>放大</name>
				<key>ZoomIn</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
			<item>
				<name>缩小</name>
				<key>ZoomOut</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
			<item>
				<name>全幅</name>
				<key>FullExtent</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
			<item>
				<name>测距</name>
				<key>Distance</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
			<item>
				<name>测面</name>
				<key>Area</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
			<item>
				<name>清除</name>
				<key>Clear</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
			<item>
				<name>全屏</name>
				<key>Full</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
		</maptool>
	</tool>
</functions>