<?xml version="1.0" encoding="UTF-8"?>
<functions>
	<!-- 预加载模块 -->
	<initwidget>
		<item>
			<name>图层控制</name>
			<key>LayerControl</key>
			<url>widgets/layercontrol/LayerControlWidget.swf</url>
			<mutex>false</mutex>
			<top>100</top>
			<right>5</right>
		</item>
		<item>
			<name>图例管理器</name>
			<key>MapLegend</key>
			<url>widgets/legend/LegendWidget.swf</url>
			<mutex>false</mutex>
			<width>280</width>
			<height>180</height>
			<bottom>0</bottom>
			<right>0</right>
		</item>
		<item>
			<name>显示控制</name>
			<key>BaseMapControl</key>
			<url>widgets/basemap/BaseMapWidget.swf</url>
			<mutex>false</mutex>
			<top>40</top>
			<right>5</right>
		</item>
	</initwidget>
	<menu>
		<mapmenu>
			<item>
				<name>水体查询</name>
				<key>WaterBodyQuery</key>
				<url>widgets/mapquery/MapQueryWidget.swf</url>
				<mutex>true</mutex>
				<init>true</init>
				<selected>true</selected>
				<subitems>
					<subitem>
						<key>WaterWatchWidget</key>
						<name>监测数据展示面板</name>
						<url>widgets/mapquery/WaterWatchIndexWidget.swf</url>
						<mutex>true</mutex>
						<cover>true</cover>
						<bottom>0</bottom>
						<left>0</left>
					</subitem>
				</subitems>
			</item>
			<item>
				<name>水质评价</name>
				<key>WaterQualityEvaluation</key>
				<url>widgets/waterevaluation/WaterEvaluationWidget.swf</url>
				<mutex>true</mutex>
				<init></init>
				<selected></selected>
			</item>
			<item>
				<name>水体预警</name>
				<key>WaterBodyWarn</key>
				<url></url>
				<mutex>true</mutex>
				<init></init>
				<selected></selected>
			</item>
		</mapmenu>
	</menu>
	<assistmenu>
	</assistmenu>
	<widgets>
		<mapsearch>
		</mapsearch>
	</widgets>
	<!--工具条 -->
	<tool>
		<maptool>
			<item>
				<name>漫游</name>
				<key>Pan</key>
				<securitycontroler>false</securitycontroler>
				<securitytype>visible</securitytype>
			</item>
			<!-- <item> -->
